<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>

      <solid
          android:color="#878b91" />
      <stroke
          android:width="1dp"
          android:color="#595a5b" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#878b91"
          android:endColor="#595a5b"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#878b91" />
    </shape>
  </item>
</selector>